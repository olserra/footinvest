<%= simple_form_for players_path, method: :get, remote: true do |f| %>
  <%= f.input :name, input_html: { name: 'name' }%>
  <%#= f.input :position, collection: [ "Goalkeeper", "Defender", "Midfielder", "Forward"] %>
  <%#= f.input :nationality %>
  <%#= f.input :age %>
  <%= f.input :tokens, as: :range, input_html: { name: 'tokens', min: '0', max: '10', step: '1'  } %>
  <%= f.button :submit, 'Spot a Star', class: 'btn-landing-blue' %>
<% end %>
